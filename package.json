{
  "name": "diesal",
  "version": "0.1.4",
  "description": "Data structures and algorithms in ES2015 modules",
  "main": "index.js",
  "scripts": {
    "prepublish": "npm run compile",
    "compile": "babel -d lib/ src/",
    "update-docs": "documentation src/**/*.js -o docs -f html",
    "test": "node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- --compilers js:babel-core/register --recursive && eslint src"
  },
  "keywords": [
    "data structures",
    "algorithms",
    "ecmascript",
    "modules"
  ],
  "author": "Jonathan Skeate",
  "license": "ISC",
  "repository": "http://github.com/skeate/diesal",
  "devDependencies": {
    "babel": "^5.8.34",
    "babel-core": "^5.8.34",
    "chai": "^3.3.0",
    "documentation": "^3.0.4",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.3",
    "mversion": "^1.10.1"
  }
}
